<odoo>
	<data>
		<!-- Sequence for petty cash -->
		<record id="seq_petty_cash" model="ir.sequence">
			<field name="name">Petty Cash</field>
			<field name="code">petty.cash</field>
			<field name="prefix">Petty/</field>
			<field name="padding">6</field>
			<field name="number_increment">1</field>
		</record>

		<record id="seq_cash_advance" model="ir.sequence">
			<field name="name">Cash Advance</field>
			<field name="code">cash.advance</field>
			<field name="prefix">Advance/</field>
			<field name="padding">6</field>
			<field name="number_increment">1</field>
		</record>
	</data>
</odoo>
